# SMTP Configuration
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your-email@example.com
SMTP_PASS=your-password
SMTP_FROM=contact@example.com

# Admin Email
ADMIN_EMAIL=admin@example.com

# SMS Provider Configuration
SMS_PROVIDER=twilio # or 'free' for Free Mobile

# Twilio Configuration (if using Twilio)
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number
TOWING_PHONE_NUMBER=towing-service-phone-number

# Free Mobile Configuration (if using Free Mobile)
FREE_MOBILE_USER=your-free-mobile-user
FREE_MOBILE_PASS=your-free-mobile-pass